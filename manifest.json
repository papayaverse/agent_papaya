{
    "manifest_version": 2,
    "name": "Papaya CookieMonster",
    "version": "1.0",
    "permissions": ["storage", "http://*/", "https://*/"],
    "content_scripts": [
      {
        "matches": ["http://*/", "https://*/"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon_papaya.png"
      }
    }
  }
  